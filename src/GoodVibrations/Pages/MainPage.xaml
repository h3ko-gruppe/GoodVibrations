<?xml version="1.0" encoding="UTF-8"?>
<xamForms:ReactiveContentPage
    x:TypeArguments="viewModels:MainViewModel"
    xmlns:xamForms="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
    xmlns:viewModels="clr-namespace:GoodVibrations.ViewModels;assembly=GoodVibrations"
    xmlns:templateSelectors="clr-namespace:GoodVibrations.TemplateSelectors;assembly=GoodVibrations"
	xmlns:styles="clr-namespace:GoodVibrations.Styles;assembly=GoodVibrations"
    xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="GoodVibrations.Pages.MainPage">
	<ContentPage.Resources>
        <ResourceDictionary MergedWith="styles:AppStyles">
          <templateSelectors:MainTemplateSelector x:Key="MainTemplateSelector" />
        </ResourceDictionary>
    </ContentPage.Resources>

	<ContentPage.Content>
		<TableView x:Name="TableView" />
    </ContentPage.Content>
</xamForms:ReactiveContentPage>
